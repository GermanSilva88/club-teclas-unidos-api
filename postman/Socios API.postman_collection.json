    {
      "name": "Consultar socio por DNI",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:8080/socios/{{createdDni}}",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8080",
          "path": [
            "socios",
            "{{createdDni}}"
          ]
        }
      },
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "pm.test(\"Response has correct DNI\", function () {",
              "    var json = pm.response.json();",
              "    pm.expect(json.dni).to.eql(pm.variables.get(\"createdDni\"));",
              "});"
            ],
            "type": "text/javascript"
          }
        }
      ]
    },
    {
      "name": "Crear socio válido",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"dni\": \"{{dni}}\",\n  \"apellido\": \"{{apellido}}\",\n  \"nombre\": \"{{nombre}}\",\n  \"edad\": {{edad}},\n  \"fechaNacimiento\": \"{{fechaNacimiento}}\",\n  \"direccion\": \"{{direccion}}\",\n  \"telefono\": \"{{telefono}}\"\n}"
        },
        "url": {
          "raw": "http://localhost:8080/socios",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8080",
          "path": [
            "socios"
          ]
        }
      },
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 201\", function () {",
              "    pm.response.to.have.status(201);",
              "});",
              "var json = pm.response.json();",
              "pm.variables.set(\"createdDni\", json.dni);",
              "pm.test(\"Response has an id\", function () {",
              "    pm.expect(json).to.have.property('id');",
              "});"
            ],
            "type": "text/javascript"
          }
        }
      ]
    },
